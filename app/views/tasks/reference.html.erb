
  <div class=wrapper>
    <div class="title">
      <h1>タスク一覧</h1>
    </div>
    <div class="task-index">
      <ul>
        <% @tasks.each do |task| %>
          <li>タスク : <%= link_to task.content, task %>,  進捗状況 :<%= link_to task.status, task %></li>
        <% end %>
      </ul>

    </div>
  </div>